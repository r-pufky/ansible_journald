[Journal]
Storage={{ journald_storage | default('auto') }}
Compress={{ journald_compress | default(true) }}
Seal={{ journald_seal | default(true) }}
SplitMode={% if journald_split_mode %}uid{% else %}none{% endif %}
SyncIntervalSec={{ journald_sync_interval_sec | default('5m') }}
RateLimitIntervalSec={{ journald_rate_limit_interval_sec | default('30s') }}
RateLimitBurst={{ journald_rate_limit_burst | default(10000) }}
SystemMaxUse={{ journald_system_max_use | default('500M') }}
SystemKeepFree={{ journald_system_keep_free | default('') }}
SystemMaxFileSize={{ journald_system_max_file_size | default('') }}
SystemMaxFiles={{ journald_system_max_files | default(100) }}
RuntimeMaxUse={{ journald_runtime_max_use | default('500M') }}
RuntimeKeepFree={{ journald_runtime_keep_free | default('') }}
RuntimeMaxFileSize={{ journald_runtime_max_file_size | default('') }}
RuntimeMaxFiles={{ journald_runtime_max_files | default(100) }}
MaxRetentionSec={{ journald_max_retention_sec | default('') }}
MaxFileSec={{ journald_max_file_sec | default('1month') }}
ForwardToSyslog={{ journald_forward_to_syslog | default(true) }}
ForwardToKMsg={{ journald_forward_to_k_msg | default(false) }}
ForwardToConsole={{ journald_forward_to_console | default(false) }}
ForwardToWall={{ journald_forward_to_wall | default(true) }}
TTYPath={{ journald_tty_path | default('/dev/console') }}
MaxLevelStore={{ journald_max_level_store | default('debug') }}
MaxLevelSyslog={{ journald_max_level_syslog | default('debug') }}
MaxLevelKMsg={{ journald_max_level_k_msg | default('notice') }}
MaxLevelConsole={{ journald_max_level_console | default('info') }}
MaxLevelWall={{ journald_max_level_wall | default('emerg') }}
LineMax={{ journald_line_max | default('48K') }}
ReadKMsg={{ journald_read_k_msg | default(true) }}
Audit={{ journald_audit | default(false) }}
